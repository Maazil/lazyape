 
 @{
     /* -----  For the calender setup ----- */
     var fullCalendarCoreMainCSS = "https://unpkg.com/@fullcalendar/core@4.0.1/main.css";
     var fullCalendarDayGridMainCSS = "https://unpkg.com/@fullcalendar/daygrid@4.0.1/main.css";

     var fullCalendarCoreMainJS = "https://unpkg.com/@fullcalendar/core@4.0.1/main.js";
     var fullCalendarDayGridMainJS = "https://unpkg.com/@fullcalendar/daygrid@4.0.1/main.js";

     /*  ----- For the month, week, day setup -------*/
     // Core
     var fullCalendarCoreJS = "https://unpkg.com/@fullcalendar/core@4.0.1/main.min.js";
     var fullCalendarCoreCSS = "https://unpkg.com/@fullcalendar/core@4.0.1/main.min.css";
     
     // Daygrid
     var fullCalendarDayGridJS = "https://unpkg.com/@fullcalendar/daygrid@4.0.1/main.min.js";
     var fullCalendarDayGridCSS = "https://unpkg.com/@fullcalendar/daygrid@4.0.1/main.min.css";
     
     // Timegrid
     var fullCalendarTimeGridJS = "https://unpkg.com/@fullcalendar/timegrid@4.0.1/main.min.js";
     var fullCalendarTimeGridCSS = "https://unpkg.com/@fullcalendar/timegrid@4.0.1/main.min.css";
     
     // Interaction
     var fullCalendarInteractionJS = "https://unpkg.com/@fullcalendar/interaction@4.0.2/main.min.js";

     // Vue 
     var fullCalenderVueJS = "https://unpkg.com/@fullcalendar/vue@4.0.2-beta/main.umd.js";
     //ll

     var fullCalendarBootstrap = "https://unpkg.com/@fullcalendar/bootstrap@4.1.0/main.min.js";    
 }
 
 @* Necessary links to css files from library fullcalendar *@
     @* These 2 links are to initialize css to calendar*@
    <link href=@fullCalendarCoreMainCSS rel="stylesheet"/>
    <link href=@fullCalendarDayGridMainCSS rel="stylesheet"/>
     @* These 3 links are for month, week and day css style*@
     <link href=@fullCalendarCoreCSS rel="stylesheet"/>
     <link href=@fullCalendarDayGridCSS rel='stylesheet'/>
    <link href=@fullCalendarTimeGridCSS rel='stylesheet'/>
 
    @* font for bootstrap link  *@
    <link href='https://use.fontawesome.com/releases/v5.0.6/css/all.css' rel='stylesheet'>
    
 @* Necessary script to js files from library *@
     @* These 2 scripts are to initialize js to calendar*@
     <script src=@fullCalendarCoreMainJS></script>
     <script src=@fullCalendarDayGridMainJS></script>
     @* These 4 scripts are to initialize js to month, week, day*@
     <script src=@fullCalendarCoreJS></script>
     <script src=@fullCalendarDayGridJS></script>
     <script src=@fullCalendarTimeGridJS></script>
     <script src=@fullCalendarInteractionJS></script>

 
 @* 
    Need to add the jquery here, since if not it will run to late for the function to run.
    Maybe not needed if the other method is working with Vue
    *@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"    
         asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
         asp-fallback-test="window.jQuery"
         crossorigin="anonymous"
         integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=">
    </script>
 
     @* script calles for getting Vue so we can use it*@
    <script src=@fullCalenderVueJS></script>
    <script src="https://unpkg.com/axios/dist/axios.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>

    @* Script to call js file and second script to call the function in the js file*@
    <script src="js/calendarVue.js"></script>
    
    @* Add bootsrapt to it *@
    <script src=@fullCalendarBootstrap></script>
 
 @* Html element her*@
 
 <div id='calendar'>
     @await Html.PartialAsync("_ModalPartial")

     @* Test buttons *@
     <div class="row">
         <button id="getTimeEdit" class=" btn btn-primary" v-on:click="TimeEditTest()" style="margin-top: 20px; margin-left: 15px;">Get Task from TimeEdit</button>
         <button id="getAutoTask" class=" btn btn-primary" v-on:click="ReloadAutoGeneratedTasks()" style="margin-top: 20px; margin-left: 15px;">Reload Auto Generated tasks</button>
     </div>

 </div>
 

 
